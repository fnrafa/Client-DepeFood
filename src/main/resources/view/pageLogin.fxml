<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="420.0" prefWidth="280.0"
            stylesheets="@../assets/css/main.css" xmlns="http://javafx.com/javafx/21"
            fx:controller="component.form.LoginForm">
    <Button layoutX="58.0" layoutY="344.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="37.0"
            prefWidth="165.0" text="Login" textAlignment="CENTER" textFill="#33272a">
        <font>
            <Font name="Nunito Regular" size="18.0"/>
        </font>
    </Button>
    <ImageView fitHeight="172.0" fitWidth="240.0" layoutX="20.0" layoutY="20.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../assets/images/image.png"/>
    </ImageView>
    <TextField fx:id="inputName" layoutX="20.0" layoutY="229.0" prefHeight="37.0" prefWidth="240.0"
               promptText="Input Name"/>
    <TextField fx:id="inputPhoneNumber" layoutX="20.0" layoutY="279.0" prefHeight="37.0" prefWidth="240.0"
               promptText="Input Phone Number"/>
    <Label layoutX="20.0" layoutY="190.0" text="DepeFood Application" textFill="#33272a">
        <font>
            <Font name="Nunito Regular" size="18.0"/>
        </font>
    </Label>
</AnchorPane>
